{
  "kind": "collectionType",
  "collectionName": "ticket_transactions",
  "info": {
    "name": "Ticket Transactions"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "TransactionsItemID": {
      "model": "transaction-items"
    },
    "TicketNumber": {
      "type": "biginteger",
      "required": true
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "to",
        "be",
        "determined"
      ]
    },
    "Amount": {
      "required": true,
      "type": "biginteger"
    }
  }
}
