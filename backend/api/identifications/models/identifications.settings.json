{
  "kind": "collectionType",
  "collectionName": "identifications",
  "info": {
    "name": "Identifications"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "sp_id": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "License",
        "Passport",
        "IdentificationCard"
      ],
      "required": true,
      "default": "License"
    },
    "agency": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "identification-images"
    }
  }
}
